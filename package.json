{
  "scripts": {
    "dev": "dotenv -c development -- tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "tsx src/index.ts",
    "lint": "biome check src/",
    "format": "biome check --apply src/",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "biome check --apply"
    ]
  },
  "dependencies": {
    "@hono/node-server": "^1.1.0",
    "@hono/zod-openapi": "^0.5.1",
    "hono": "^3.6.0",
    "znv": "^0.4.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@types/node": "^20.6.3",
    "dotenv-cli": "^7.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "tsx": "^3.12.2"
  },
  "volta": {
    "node": "18.18.0",
    "npm": "10.1.0"
  }
}
